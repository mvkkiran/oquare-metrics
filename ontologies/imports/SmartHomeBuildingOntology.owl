###  http://elite.polito.it/ontologies/dogont.owl#Switch
dogont:Switch rdf:type owl:Class ;
              owl:equivalentClass bosh:Switch .


###  http://elite.polito.it/ontologies/dogont.owl#TemperatureSensor
dogont:TemperatureSensor rdf:type owl:Class ;
                         owl:equivalentClass bosh:TemperatureSensor .


###  http://purl.org/bosh#Acceleration
bosh:Acceleration rdf:type owl:Class ;
                  rdfs:subClassOf bosh:QuantityKind ;
                  rdfs:comment "The rate of change of the velocity of a particle with respect to time."@en ;
                  rdfs:label "Acceleration"@en ;
                  rdfs:seeAlso <http://purl.oclc.org/NET/ssnx/qu/dim#Acceleration> .


###  http://purl.org/bosh#AccelerationInstantaneous
bosh:AccelerationInstantaneous rdf:type owl:Class ;
                               rdfs:subClassOf bosh:Acceleration ;
                               rdfs:comment "The acceleration at a given instant of time."@en ;
                               rdfs:label "Instantaneous Acceleration"@en .


###  http://purl.org/bosh#Accelerometer
bosh:Accelerometer rdf:type owl:Class ;
                   owl:equivalentClass m3lite:Accelerometer ;
                   rdfs:subClassOf bosh:SensingDevice ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty bosh:hasDomainOfInterest ;
                                     owl:someValuesFrom bosh:BuildingAutomation
                                   ] ;
                   rdfs:comment " Accelerometers are used to automatically determine the orientation in which the user is holding the IoT Object (portrait or landscape)"@en ;
                   rdfs:label "Accelerometer"@en ;
                   rdfs:seeAlso <http://casas.wsu.edu/owl/cose.owl#Accelerometer> .


###  http://purl.org/bosh#AccessControlSystem
bosh:AccessControlSystem rdf:type owl:Class ;
                         rdfs:subClassOf bosh:SecuritySystem ;
                         rdfs:comment "System that controls access to certain devices, environment or places."@en ;
                         rdfs:label "Access Control System"@en .


###  http://purl.org/bosh#ActuatingDevice
bosh:ActuatingDevice rdf:type owl:Class ;
                     owl:equivalentClass saref:Actuator ;
                     rdfs:subClassOf bosh:Device ;
                     rdfs:comment "Device that can actuate over an object or QuantityKind (e.g., switch on/off the light)."@en ;
                     rdfs:label "Actuating Device"@en .


###  http://purl.org/bosh#AirConditioner
bosh:AirConditioner rdf:type owl:Class ;
                    owl:equivalentClass m3lite:AirConditioner ;
                    rdfs:subClassOf bosh:ActuatingDevice ,
                                    bosh:AirConditioningSystem ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty bosh:hasDomainOfInterest ;
                                      owl:someValuesFrom bosh:BuildingAutomation
                                    ] ;
                    rdfs:comment "An actuator to automatically switch on/off the air conditioner."@en ;
                    rdfs:label "Air Conditioner"@en .


###  http://purl.org/bosh#AirConditioningSystem
bosh:AirConditioningSystem rdf:type owl:Class ;
                           rdfs:subClassOf bosh:ActuatingDevice ,
                                           bosh:HVAC ;
                           rdfs:comment "A conditioning system/utility to keep a room/building cool or dry."@en ;
                           rdfs:label "Air Conditioning System"@en .


###  http://purl.org/bosh#AirFlowSensor
bosh:AirFlowSensor rdf:type owl:Class ;
                   rdfs:subClassOf bosh:SensingDevice ;
                   rdfs:comment "Device used to detect the Air Flow."@en ;
                   rdfs:label "Air Flow Sensor"@en .


###  http://purl.org/bosh#AirHumiditySensor
bosh:AirHumiditySensor rdf:type owl:Class ;
                       rdfs:subClassOf bosh:HumiditySensor ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty bosh:hasDomainOfInterest ;
                                         owl:someValuesFrom bosh:Environment
                                       ] ;
                       rdfs:comment "Sensor used to measure air humidity."@en ;
                       rdfs:label "Air Humidity Sensor"@en .


###  http://purl.org/bosh#AirPollution
bosh:AirPollution rdf:type owl:Class ;
                  rdfs:subClassOf bosh:QuantityKind ;
                  rdfs:comment "Usually measured using Air Quality Index (AQI), it is the measure of Air Pollution in the environment. It is similar to Air Quality."@en ;
                  rdfs:label "Air Pollution Quantity Kind"@en .


###  http://purl.org/bosh#AirPollutionSensor
bosh:AirPollutionSensor rdf:type owl:Class ;
                        owl:equivalentClass m3lite:AirPollutantSensor ;
                        rdfs:subClassOf bosh:SensingDevice ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty bosh:hasDomainOfInterest ;
                                          owl:someValuesFrom bosh:Environment
                                        ] ;
                        rdfs:comment "Air Pollution Sensor are devices that detect and monitor the presence of air pollution in the surrounding area."@en ;
                        rdfs:label "Air Pollution Sensor"@en .


###  http://purl.org/bosh#AirQuality
bosh:AirQuality rdf:type owl:Class ;
                rdfs:subClassOf bosh:QuantityKind ;
                rdfs:comment "Usually measured using an air quality index (AQI) that is a number used by government agencies to communicate to the public how polluted the air currently is or how polluted it is forecast to become. Different countries have their own air quality indices, corresponding to different national air quality standards. It is the measure of Air Quality of the environment. It is similar to Air Pollution. (Source Wikipedia)."@en ;
                rdfs:label "Air Quality Quantity Kind"@en .


###  http://purl.org/bosh#AirTemperature
bosh:AirTemperature rdf:type owl:Class ;
                    rdfs:subClassOf bosh:Temperature ;
                    rdfs:comment "The temperature of the air that would be indicated by a thermometer exposed to the air at a location sheltered from direct solar radiation."@en ;
                    rdfs:label "Air Temperature, Weather Temperature, Ambient Temperature"@en ;
                    rdfs:seeAlso <http://openiot.eu/ontology/ns/AirTemperature> ,
                                 <http://paul.staroch.name/thesis/SmartHomeWeather.owl#Temperature> .


###  http://purl.org/bosh#AirThermometer
bosh:AirThermometer rdf:type owl:Class ;
                    rdfs:subClassOf bosh:Thermometer ;
                    rdfs:comment "Device to measure the Air temperature of either indoor or outdoor atmosphere."@en ;
                    rdfs:label "Air Thermometer"@en .


###  http://purl.org/bosh#Alarm
bosh:Alarm rdf:type owl:Class ;
           owl:equivalentClass bosh:Siren ,
                               m3lite:AlarmSystem ;
           rdfs:subClassOf bosh:ActuatingDevice ;
           rdfs:comment "An actuator to automatically switch on/off the alarm system."@en ;
           rdfs:label "Alarm System Actuating Device"@en ;
           skos:altLabel "Siren"@en ,
                         "Warning Alarm"@en ,
                         "Warning Alert"@en .


###  http://purl.org/bosh#Ampere
bosh:Ampere rdf:type owl:Class ;
            rdfs:subClassOf bosh:Unit ;
            rdfs:comment "The ampere is the SI unit for measuring an electric current which is the flow of electric charges through a surface at the rate of one coulomb per second.The ampere is the SI unit for measuring an electric current which is the flow of electric charges through a surface at the rate of one coulomb per second."@en ;
            rdfs:label "Ampere (A)"@en ;
            rdfs:seeAlso <http://data.qudt.org/qudt/owl/1.0.0/unit.owl#Ampere> ,
                         <http://purl.oclc.org/NET/ssnx/qu/unit#ampere> ,
                         <http://sweet.jpl.nasa.gov/ontology/units.owl#ampere> .


###  http://purl.org/bosh#AngularSpeed
bosh:AngularSpeed rdf:type owl:Class ;
                  rdfs:subClassOf bosh:QuantityKind ;
                  rdfs:comment "It is the rate of change of angular displacement and is a vector quantity (more precisely, a pseudovector) that specifies the angular speed (rotational speed) of an object and the axis about which the object is rotating. (Source Wikipedia)."@en ;
                  rdfs:label "Angular Speed"@en .


###  http://purl.org/bosh#Appliance
bosh:Appliance rdf:type owl:Class ;
               owl:equivalentClass saref:Appliance ;
               rdfs:comment "An electical/electronic or mechanical house hold machine that can achieve a function such as cleaning, cooking, washing etc."@en ;
               rdfs:label "Appliance"@en .


###  http://purl.org/bosh#ApplicationDomain
bosh:ApplicationDomain rdf:type owl:Class ;
                       owl:equivalentClass m3lite:DomainOfInterest ;
                       rdfs:comment "Application Domain for example: smart home, environment, etc."@en ;
                       rdfs:label "Application Domain"@en ;
                       skos:altLabel "Domain of Interest"@en .


###  http://purl.org/bosh#AtmosphericPressure
bosh:AtmosphericPressure rdf:type owl:Class ;
                         rdfs:subClassOf bosh:Pressure ;
                         rdfs:comment "The pressure exerted by the atmosphere as a consequence of gravitational attraction exerted upon the column of air lying directly above the point in question."@en ;
                         rdfs:label "Atmospheric Pressure"@en ;
                         rdfs:seeAlso <http://openiot.eu/ontology/ns/AtmospherePressure> ,
                                      <http://paul.staroch.name/thesis/SmartHomeWeather.owl#AtmosphericPressure> ,
                                      "http://purl.oclc.org/NET/ssnx/qu/quantity#atmosphericPressure" .


###  http://purl.org/bosh#AtmosphericPressureSensor
bosh:AtmosphericPressureSensor rdf:type owl:Class ;
                               owl:equivalentClass m3lite:AtmosphericPressureSensor ;
                               rdfs:subClassOf bosh:SensingDevice ;
                               rdfs:comment "Atmospheric Pressure Sensor, Barometer or Barometric Pressure Sensor is a scientific instrument used in meteorology to measure atmospheric pressure."@en ;
                               rdfs:label "Atmospheric Pressure Sensor"@en ;
                               rdfs:seeAlso <http://archive.knoesis.org/research/semsci/application_domain/sem_sensor/ont/weather.owlPressure> ,
                                            <http://casas.wsu.edu/owl/cose.owl#BarometricPressureSensor> ,
                                            <http://purl.oclc.org/NET/ssnx/meteo/aws#AtmosphericPressureSensor> .


###  http://purl.org/bosh#Balcony
bosh:Balcony rdf:type owl:Class ;
             rdfs:comment "A platform projecting from the wall of a building and surrounded by railing or half wall."@en ;
             rdfs:label "Balcony"@en .


###  http://purl.org/bosh#Barcode
bosh:Barcode rdf:type owl:Class ;
             rdfs:subClassOf bosh:SmartTagDevice ;
             rdfs:comment "A tagging device can be Barcode."@en ;
             rdfs:label "Barcode Tagging Device"@en .


###  http://purl.org/bosh#Bathroom
bosh:Bathroom rdf:type owl:Class ;
              rdfs:subClassOf bosh:Room ;
              rdfs:comment ""@en ;
              rdfs:label "Bathroom"@en .


###  http://purl.org/bosh#BatteryLevel
bosh:BatteryLevel rdf:type owl:Class ;
                  rdfs:subClassOf bosh:QuantityKind ;
                  rdfs:comment "Measure of the percentage of the battery capacity that remains available."@en ;
                  rdfs:label "Battery Level Quantity Kind"@en ;
                  rdfs:seeAlso <http://www.w3.org/2007/uwa/context/deliveryContext.owl#BatteryLevel> .


###  http://purl.org/bosh#BedRoom
bosh:BedRoom rdf:type owl:Class ;
             rdfs:subClassOf bosh:Room ;
             rdfs:comment ""@en ;
             rdfs:label "Bedroom"@en .


###  http://purl.org/bosh#BluetoothComponent
bosh:BluetoothComponent rdf:type owl:Class ;
                        rdfs:subClassOf bosh:WirelessComponent ;
                        rdfs:comment "A bluetooth device."@en ;
                        rdfs:label "Bluetooth Component"@en ;
                        skos:altLabel "Bluetooth"@en .


###  http://purl.org/bosh#BoardTemperature
bosh:BoardTemperature rdf:type owl:Class ;
                      rdfs:subClassOf bosh:Temperature ;
                      rdfs:comment "Measure of board temperature."@en ;
                      rdfs:label "Board Temperature"@en .


###  http://purl.org/bosh#BoardThermometer
bosh:BoardThermometer rdf:type owl:Class ;
                      owl:equivalentClass m3lite:BoardThermometer ;
                      rdfs:subClassOf bosh:Thermometer ;
                      rdfs:comment "Device used to measure board temperature."@en ;
                      rdfs:label "Board Thermometer"@en .


###  http://purl.org/bosh#BodyTemperature
bosh:BodyTemperature rdf:type owl:Class ;
                     rdfs:subClassOf bosh:Temperature ;
                     rdfs:comment "Measure of body temperature."@en ;
                     rdfs:label "Body Temperature"@en .


###  http://purl.org/bosh#Boiler
bosh:Boiler rdf:type owl:Class ;
            owl:equivalentClass m3lite:Boiler ;
            rdfs:subClassOf bosh:ActuatingDevice ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty bosh:hasDomainOfInterest ;
                              owl:someValuesFrom bosh:BuildingAutomation
                            ] ;
            rdfs:comment "An actuator to automatically switch on/off the boiler."@en ;
            rdfs:label "Boiler"@en .


###  http://purl.org/bosh#Building
bosh:Building rdf:type owl:Class ;
              rdfs:subClassOf bosh:BuildingEnvironment ;
              rdfs:comment "A civil engineering structure with roof and walls, such as house, factory, workplace etc."@en ;
              rdfs:label "Building"@en .


###  http://purl.org/bosh#BuildingAutomation
bosh:BuildingAutomation rdf:type owl:Class ;
                        owl:equivalentClass m3lite:BuildingAutomation ;
                        rdfs:subClassOf bosh:ApplicationDomain ;
                        rdfs:comment "Smart Home/Building Automation as an Internet of Things (IoT) applicative domain."@en ;
                        rdfs:label "Building Automation"@en ;
                        rdfs:seeAlso dogont:BuildingEnvironment .


###  http://purl.org/bosh#BuildingEnvironment
bosh:BuildingEnvironment rdf:type owl:Class ;
                         rdfs:subClassOf bosh:Environment ;
                         rdfs:comment "A living space or environment for people."@en ;
                         rdfs:label "Building Environment"@en ;
                         skos:altLabel "Building Space"@en .


###  http://purl.org/bosh#BuildingTemperature
bosh:BuildingTemperature rdf:type owl:Class ;
                         rdfs:subClassOf bosh:Temperature ;
                         rdfs:comment "Measure of building or room temperature."@en ;
                         rdfs:label "Building/Room Temperature"@en .


###  http://purl.org/bosh#Button
bosh:Button rdf:type owl:Class ;
            rdfs:subClassOf bosh:Control ;
            rdfs:comment "A button to turn on or off a device."@en ;
            rdfs:label "Button"@en .


###  http://purl.org/bosh#CO2
bosh:CO2 rdf:type owl:Class ;
         rdfs:subClassOf bosh:QuantityKind ;
         rdfs:comment "Carbon Dioxide (CO2) measure in the atmosphere."@en ;
         rdfs:label "Carbon Dioxide"@en .


###  http://purl.org/bosh#CO2Sensor
bosh:CO2Sensor rdf:type owl:Class ;
               owl:equivalentClass m3lite:CO2Sensor ;
               rdfs:subClassOf bosh:GaseousPollutionSensor ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty bosh:hasDomainOfInterest ;
                                 owl:someValuesFrom bosh:Environment
                               ] ;
               rdfs:comment "Carbon Dioxide (CO2) Sensor used to measure level of CO2 in the atmosphere."@en ;
               rdfs:label "Carbon Dioxide (CO2) Sensor"@en .


###  http://purl.org/bosh#COSensor
bosh:COSensor rdf:type owl:Class ;
              owl:equivalentClass m3lite:COSensor ;
              rdfs:subClassOf bosh:GaseousPollutionSensor ;
              rdfs:comment "Device to detect Carbon Monoxide (CO) in the environment."@en ;
              rdfs:label "Carbon Monoxide (CO) Sensor"@en .


###  http://purl.org/bosh#CalibrationSensing
bosh:CalibrationSensing rdf:type owl:Class ;
                        rdfs:subClassOf bosh:MeasurementType ;
