@prefix saref: <https://w3id.org/saref#> .
@prefix dogont: <http://elite.polito.it/ontologies/dogont.owl#> .
@prefix m3lite: <http://purl.org/iot/vocab/m3-lite#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://purl.org/bosh> .

<http://purl.org/bosh> rdf:type owl:Ontology ;
                        dcterms:created "2021-07-14" ;
                        dcterms:creator "Amelie Gyrard"@en ,
                                        "Mahda Noura"@en ,
                                        "Ramesh Gowtham"@en ,
                                        "Vamsee Krishna Kiran Muppavarapu"@en ;
                        dcterms:description "Ontology covering the concepts related to smart home and smart building domains. Ontology contains sensors, actuators, building entities, measurements and so on."@en ;
                        dcterms:modified "2021-11-30" ;
                        dcterms:title "Smart Home and Building Ontology (BOSH)"@en ;
                        rdfs:comment "Smart Home and Building Upper Ontology developed as part of PhD research work."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/bosh#belongsTo
bosh:belongsTo rdf:type owl:ObjectProperty ;
               rdfs:domain bosh:Control ;
               rdfs:range bosh:Device ,
                          bosh:SecuritySystem ;
               rdfs:comment "Property used to map the controls like switches, buttons etc with the devices."@en ;
               rdfs:label "belongs to"@en .


###  http://purl.org/bosh#contains
bosh:contains rdf:type owl:ObjectProperty ;
              rdfs:domain bosh:BuildingEnvironment ;
              rdfs:range bosh:Appliance ,
                         bosh:BuildingAutomation ,
                         bosh:BuildingEnvironment ,
                         bosh:Device ,
                         bosh:SecuritySystem ;
              rdfs:comment "This property is used to map all building related items."@en ;
              rdfs:label "contains"@en .


###  http://purl.org/bosh#hasActuator
bosh:hasActuator rdf:type owl:ObjectProperty ;
                 rdfs:domain bosh:Appliance ,
                             bosh:BuildingAutomation ;
                 rdfs:range bosh:ActuatingDevice ;
                 rdfs:comment "Property used to map actuators with the respective classes."@en ;
                 rdfs:label "has Actuator"@en .


###  http://purl.org/bosh#hasDomainOfInterest
bosh:hasDomainOfInterest rdf:type owl:ObjectProperty ;
                         rdfs:range bosh:ApplicationDomain ;
                         rdfs:comment "This property is used to classify devices by DomainOfInterest (e.g., blood pressure sensor is used in healthcare)."@en ;
                         rdfs:label "has Domain Of Interest"@en .


###  http://purl.org/bosh#hasMeasurementType
bosh:hasMeasurementType rdf:type owl:ObjectProperty ;
                        rdfs:range [ rdf:type owl:Restriction ;
                                     owl:onProperty bosh:hasMeasurementType ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass bosh:MeasurementType
                                   ] ;
                        rdfs:comment "Each sensing device can have a different sensing mechanism which may result in different kinds of sensor data. This property links observation of the sensor to the associated MeasurementType."@en ;
                        rdfs:label "has Measurement Type"@en .


###  http://purl.org/bosh#hasSensor
bosh:hasSensor rdf:type owl:ObjectProperty ;
               rdfs:domain bosh:Appliance ,
                           bosh:BuildingAutomation ;
               rdfs:range bosh:SensingDevice ;
               rdfs:comment "Property used to map sensors with the respective classes."@en ;
               rdfs:label "has Sensor"@en .


#################################################################
#    Classes
#################################################################

###  http://elite.polito.it/ontologies/dogont.owl#Actuator
dogont:Actuator rdf:type owl:Class ;
